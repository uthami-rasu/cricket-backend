{
  "name": "cricket",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node app.js --host 0.0.0.0",
    "dev": "nodemon index.js --host 0.0.0.0",
    "db:migrate": "npx sequelize-cli db:migrate --config src/config/config.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all --config src/config/config.js",
    "db:seed": "npx sequelize-cli db:seed:all --config src/config/config.js",
    "docker:up": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "test": "jest --passWithNoTests"
  },
